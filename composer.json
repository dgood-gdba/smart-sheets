{
    "name": "gdba/smart-sheets",
    "description": "PHP integration to SmartSheetsAPI",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "David Good",
            "email": "dgood@gdba.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "nesbot/carbon": "^2.46.0"
    },
    "autoload": {
        "psr-4": {
            "GDBA\\SmartSheets\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "GDBA\\SmartSheets\\SmartSheetsServiceProvider"
            ],
            "aliases": {
                "SmartSheets": "GDBA\\SmartSheets\\Facades\\SmartSheets"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
